    <div class="visit-counter">
        Visits: <span id="visitCount">0</span> | Interactions: <span id="interactionCount">0</span>
    </div>

    <script>
        let userResponses = {};
        const totalQuestions = 13;
        let interactionCount = 0;

        // Initialize tracking
        function initTracking() {
            let visits = localStorage.getItem('bodyPositivityVisits') || 0;
            visits = parseInt(visits) + 1;
            localStorage.setItem('bodyPositivityVisits', visits);
            document.getElementById('visitCount').textContent = visits;
            
            // Track page load
            trackInteraction('page_load');
        }

        function trackInteraction(action) {
            interactionCount++;
            document.getElementById('interactionCount').textContent = interactionCount;
            
            // Store interaction data
            const interactions = JSON.parse(localStorage.getItem('interactions') || '[]');
            interactions.push({
                action: action,
                timestamp: new Date().toISOString(),
                responses: userResponses
            });
            localStorage.setItem('interactions', JSON.stringify(interactions));
        }

        function updateProgress(questionNumber) {
            const percentage = ((questionNumber - 1) / totalQuestions) * 100;
            document.getElementById('progressBar').style.width = percentage + '%';
        }

        function startJourney() {
            trackInteraction('journey_started');
            showQuestion(1);
        }

        function showQuestion(questionNumber) {
            // Hide all sections
            document.querySelectorAll('.question-section').forEach(section => {
                section.classList.remove('active');
            });
            
            // Show specific question
            if (questionNumber === 'results') {
                document.getElementById('results').classList.add('active');
                updateProgress(totalQuestions + 1);
            } else {
                document.getElementById('q' + questionNumber).classList.add('active');
                updateProgress(questionNumber);
            }
            
            trackInteraction('question_' + questionNumber + '_viewed');
        }

        function saveAnswer(key, value, nextQuestion) {
            userResponses[key] = value;
            trackInteraction('answered_' + key + '_with_' + value);
            
            if (nextQuestion === 'results') {
                showResults();
            } else {
                showQuestion(nextQuestion);
            }
        }

        function showOtherInput() {
            document.getElementById('otherFeelingInput').style.display = 'block';
        }

        function saveOtherFeeling() {
            const otherValue = document.getElementById('otherFeeling').value;
            if (otherValue.trim()) {
                userResponses['feeling'] = otherValue;
                trackInteraction('custom_feeling_entered');
                showQuestion(5);
            }
        }

        function goBack(previousQuestion) {
            trackInteraction('went_back_to_q' + previousQuestion);
            showQuestion(previousQuestion);
        }

        function showResults() {
            showQuestion('results');
            generatePersonalizedResources();
        }

        function generatePersonalizedResources() {
            let html = '<div style="line-height: 1.8; color: #666;">';
            
            // Personalized greeting based on mood
            if (userResponses.feeling === 'sad') {
                html += '<p style="font-size: 18px; color: #B31B1B; margin-bottom: 20px; text-align: center;"><strong>We hear you, and we\'re here for you.</strong> It takes courage to acknowledge how you\'re feeling. You\'ve taken an important step today. üíï</p>';
            } else if (userResponses.feeling === 'happy') {
                html += '<p style="font-size: 18px; color: #B31B1B; margin-bottom: 20px; text-align: center;"><strong>We\'re so glad you\'re in a good place!</strong> Let\'s build on that positive energy together. üåü</p>';
            } else {
                html += '<p style="font-size: 18px; color: #B31B1B; margin-bottom: 20px; text-align: center;"><strong>Thank you for being here.</strong> Every step forward is a victory worth celebrating! üéâ</p>';
            }

            // Core Competencies Highlight
            html += '<div class="core-competencies">';
            html += '<h3>üåü The Five Core Competencies of Body Positivity üåü</h3>';
            html += '<ul class="competency-list">';
            html += '<li><strong>1. Reclaiming Health</strong> - Health at every size, focusing on wellbeing not weight</li>';
            html += '<li><strong>2. Practicing Intuitive Self-Care</strong> - Listening to your body\'s needs with compassion</li>';
            html += '<li><strong>3. Cultivating Self-Love</strong> - Embracing yourself with kindness and acceptance</li>';
            html += '<li><strong>4. Declaring Your Own Authentic Beauty</strong> - Defining beauty on your own terms</li>';
            html += '<li><strong>5. Building Community</strong> - Finding support and connection with others</li>';
            html += '</ul>';
            html += '</div>';

            // Loneliness support
            if (userResponses.lonely === 'yes') {
                html += '<div class="resource-card" style="background: #fff0f0; border-color: #B31B1B;">';
                html += '<h3 style="color: #B31B1B;">ü§ù You\'re Not Alone - Connect with Cornell Community!</h3>';
                html += '<p>Feeling lonely is completely normal, and there are so many ways to connect:</p>';
                html += '<p><strong>üìÖ <a href="https://cornell.campusgroups.com/events" style="color: #B31B1B;">Cornell Campus Events</a></strong> - Discover events happening across campus and join groups that interest you!</p>';
                html += '<p>There are hundreds of student organizations waiting to welcome you. You belong here! üêª</p>';
                html += '</div>';
            }

            // Cornell-specific resources
            if (userResponses.cornell === 'yes') {
                html += '<div class="resource-card">';
                html += '<h3 style="color: #B31B1B;">üèõÔ∏è Cornell Resources Just for You:</h3>';
                
                // Nutrition support for food-related concerns
                if (userResponses.food_relationship === 'challenging' || userResponses.eating_difficulty === 'yes') {
                    html += '<p><strong>üçé Nutrition Support:</strong> Cornell provides nutrition consultations for students on insurance for just <strong>$10/visit</strong>!</p>';
                    html += '<p>üìç <a href="https://health.cornell.edu/services/nutrition-services" style="color: #B31B1B;">Cornell Nutrition Services</a></p>';
                }
                
                // Mental health support
                if (userResponses.feeling === 'sad' || userResponses.body_feelings === 'struggling') {
                    html += '<p><strong>üíö Mental Health Support:</strong> Counseling services available for <strong>$10/visit</strong> with insurance.</p>';
                    html += '<p>üìû Call Cornell Health at <strong>607-255-5155</strong> during business hours to schedule an appointment.</p>';
                    html += '<p>üìç <a href="https://health.cornell.edu/services/counseling-psychiatry" style="color: #B31B1B;">Cornell Counseling Services</a></p>';
                }
                
                html += '</div>';
            }

            // Body Positive Resources for Everyone
            html += '<div class="resource-card" style="background: #f0f9ff;">';
            html += '<h3 style="color: #B31B1B;">üìö Body Positive Resources & Programs:</h3>';
            html += '<p><strong>üåü <a href="https://health.cornell.edu/initiatives/campus-initiatives/body-positive-cornell" style="color: #B31B1B;">Body Positive Cornell</a></strong> - Explore all our body positivity resources!</p>';
            html += '<p><strong>üìñ <a href="https://thebodypositive.org/fundamentals/" style="color: #B31B1B;">Be Body Positive‚Ñ¢ Curriculum</a></strong> - Learn the foundational principles</p>';
            html += '<p><strong>‚úçÔ∏è <a href="https://cornell.ca1.qualtrics.com/jfe/form/SV_8waQwrC2CU8iruS" style="color: #B31B1B;">Sign Up for Body Positive Fundamentals</a></strong> - Join our transformative program!</p>';
            html += '</div>';

            // Personalized messages based on specific concerns
            if (userResponses.body_influence === 'always' || userResponses.body_influence === 'most_time') {
                html += '<div class="resource-card" style="background: #fff9f0;">';
                html += '<h3 style="color: #B31B1B;">üí´ Breaking Free from Body Image Constraints</h3>';
                html += '<p>You deserve to live fully, regardless of how you look. Your body is not an ornament to be admired - it\'s the vehicle for your amazing life! Consider:</p>';
                html += '<ul style="list-style: none; padding: 0;">';
                html += '<li>üå± Practice one activity this week that you\'ve avoided due to body concerns</li>';
                html += '<li>üé® Focus on what your body can DO rather than how it looks</li>';
                html += '<li>üí™ Celebrate your body\'s strength and resilience</li>';
                html += '</ul>';
                html += '</div>';
            }

            if (userResponses.eaten === 'no') {
                html += '<div class="resource-card" style="background: #fff0f0; border: 2px solid #B31B1B;">';
                html += '<h3 style="color: #B31B1B;">üçΩÔ∏è Gentle Reminder: Nourishment Matters</h3>';
                html += '<p>Your body needs fuel to support you through your day. Even a small snack can make a difference. Cornell dining halls offer diverse options to meet all needs. Remember: eating is self-care! ü•ó</p>';
                html += '</div>';
            }

            if (userResponses.food_thoughts === 'yes') {
                html += '<div class="resource-card">';
                html += '<h3 style="color: #B31B1B;">üß† Finding Food Freedom</h3>';
                html += '<p>Constant thoughts about food can be exhausting. You\'re not alone in this struggle. Intuitive eating can help you find peace with food. The Body Positive program teaches how to:</p>';
                html += '<ul style="list-style: none; padding: 0;">';
                html += '<li>‚ú® Trust your body\'s hunger and fullness cues</li>';
                html += '<li>üéØ Eat for satisfaction and nourishment</li>';
                html += '<li>üíö Release guilt around food choices</li>';
                html += '</ul>';
                html += '</div>';
            }

            // Gender-specific encouragement
            if (userResponses.gender === 'lgbtq+') {
                html += '<div class="resource-card" style="background: linear-gradient(135deg, #fff0f0, #f0f0ff);">';
                html += '<h3 style="color: #B31B1B;">üè≥Ô∏è‚Äçüåà LGBTQ+ Affirming Support</h3>';
                html += '<p>Your identity is celebrated here! Body image can be especially complex in LGBTQ+ communities, and we understand. You deserve support that honors all aspects of who you are.</p>';
                html += '</div>';
            }

            // Support system encouragement
            if (userResponses.discussed === 'no') {
                html += '<div class="resource-card">';
                html += '<h3 style="color: #B31B1B;">üí¨ Opening Up Can Help</h3>';
                html += '<p>Sharing your journey with trusted friends or family can provide invaluable support. If you\'re not ready for that, Cornell\'s counseling services offer a safe, confidential space to explore these feelings. You don\'t have to do this alone! ü§ó</p>';
                html += '</div>';
            }

            // Final encouragement
            html += '<div style="background: linear-gradient(135deg, #fff5f5, #ffe0e0); padding: 20px; border-radius: 15px; margin-top: 20px; text-align: center;">';
            html += '<p style="color: #B31B1B; font-size: 20px; font-weight: 600; margin-bottom: 15px;">Remember: You Are Enough, Just As You Are üêª</p>';
            html += '<p style="color: #666; line-height: 1.8;">Every body is a good body. Every day is a new opportunity to practice self-compassion. You\'ve shown incredible strength by seeking support - that\'s BEAR-y brave! Keep going, one paw-step at a time.</p>';
            html += '<p style="margin-top: 15px; font-style: italic;">Life isn\'t about waiting for the storm to pass...<br>It\'s about learning to dance in the rain! üåà</p>';
            html += '</div>';
            
            html += '</div>';
            
            document.getElementById('resultsContent').innerHTML = html;
        }

        function restart() {
            trackInteraction('restarted_journey');
            userResponses = {};
            interactionCount = 0;
            document.querySelectorAll('.question-section').forEach(section => {
                section.classList.remove('active');
            });
            document.getElementById('welcome').classList.add('active');
            document.getElementById('progressBar').style.width = '0%';
            document.getElementById('otherFeeling').value = '';
            document.getElementById('otherFeelingInput').style.display = 'none';
        }

        // Initialize when page loads
        window.onload = function() {
            initTracking();
        };
    </script>
</body>
</html>
